# chrome.manifest for development
content	selenium-ide	content/ xpcnativewrappers=yes
content	selenium-ide-testrunner	content/ xpcnativewrappers=no
overlay	chrome://browser/content/browser.xul	chrome://selenium-ide/content/selenium-ide-overlay.xul
locale selenium-ide en-US locale/en-US/
locale selenium-ide fr-FR locale/fr-FR/
locale selenium-ide ja-JP locale/ja-JP/
locale selenium-ide pt-BR locale/pt-BR/
skin selenium-ide classic/1.0 skin/classic/
