    ) else (
      echo Missing IE 32-bit native binary, copying from Win32 prebuilt location $(SolutionDir)cpp\prebuilt\Win32\Release\IEDriver.dll
      copy /y "$(SolutionDir)cpp\prebuilt\Win32\Release\IEDriver.dll" "$(TargetDir)" &gt; NUL
    )
    goto end
  )
)

echo Detected 64-bit platform
if exist "$(SolutionDir)build\cpp\x64\$(ConfigurationName)\IEDriver.dll" (
  echo Found IE 64-bit native binary, copying from x64 build target $(SolutionDir)build\cpp\x64\$(ConfigurationName)\IEDriver.dll
  copy /y "$(SolutionDir)build\cpp\x64\$(ConfigurationName)\IEDriver.dll" "$(TargetDir)" &gt; NUL
) else (
  echo Missing IE 64-bit native binary, copying from x64 prebuilt location $(SolutionDir)cpp\prebuilt\x64\Release\IEDriver.dll
  copy /y "$(SolutionDir)cpp\prebuilt\x64\Release\IEDriver.dll" "$(TargetDir)" &gt; NUL
)

:end
