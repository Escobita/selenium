    <PostBuildEvent>if %25PROCESSOR_ARCHITECTURE%25 == x86 (
  if not defined PROCESSOR_ARCHITEW6432 (
    echo Detected 32-bit platform
    if exist "$(SolutionDir)build\cpp\Win32\$(ConfigurationName)\IEDriver.dll" (
      echo Found IE 32-bit native binary, copying from Win32 build target $(SolutionDir)build\cpp\Win32\$(ConfigurationName)\IEDriver.dll
